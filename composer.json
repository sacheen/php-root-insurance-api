{
    "name": "sacheen/php-root-insurance-api",
    "type": "library",
    "description": "A Wrapper for the Root Insurance API",
    "keywords": [
        "sacheen",
        "php-root-insurance-api"
    ],
    "homepage": "https://github.com/sacheen/php-root-insurance-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Sacheen",
            "email": "sacheend@funktion-it.co.za",
            "homepage": "https://github.com/sacheen",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "SD\\Root\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SD\\Root\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
